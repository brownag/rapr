citHeader("To cite rapr in publications use:")


citEntry(
  entry    = "Manual",
  title    = "rapr: Interface to Rangeland Analysis Platform (RAP) Vegetation Biomass and Cover Products",
  author   = "Andrew Brown",
  url      = "http://github.com/brownag/rapr",
  textVersion = paste(
  "Andrew Brown (2022). rapr: Interface to Rangeland Analysis Platform (RAP) Vegetation Biomass and Cover Products. R package version 0.1.0. http://github.com/brownag/rapr"
  )
)


citEntry(
  entry    = "Article",
  title    = "Annual and 16-Day Rangeland Production Estimates for the Western United States.",
  author   = "Jones, M.O., N.P. Robinson, D.E. Naugle, J.D. Maestas, M.C. Reeves, R.W.
Lankston, and B.W. Allred.",
  journal  = "Rangeland Ecology & Management",
  year     = "2021",
  volume   = 77,
  pages    = "112-117",
  url      = "http://dx.doi.org/10.1016/j.rama.2021.04.003",
  textVersion = paste(
"Jones, M.O., N.P. Robinson, D.E. Naugle, J.D. Maestas, M.C. Reeves, R.W.
Lankston, and B.W. Allred. 2021. Annual and 16-Day Rangeland Production
Estimates for the Western United States. Rangeland Ecology & Management
77:112-117. http://dx.doi.org/10.1016/j.rama.2021.04.003"
  )
)


